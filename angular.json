{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "healthnable-foundation-app": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/healthnable-foundation-app",
            "sourceRoot": "projects/healthnable-foundation-app/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/healthnable-foundation-app",
                        "index": "projects/healthnable-foundation-app/src/index.html",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "projects/healthnable-foundation-app/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [{
                            "glob": "**/*",
                            "input": "projects/healthnable-foundation-app/public",
                            "output": "."
                        }],
                        "styles": [
                            "@angular/material/prebuilt-themes/azure-blue.css",
                            "projects/healthnable-foundation-app/src/styles.scss",
                            "node_modules/bootstrap/scss/bootstrap.scss",
                            "node_modules/bootstrap/dist/css/bootstrap.min.css",
                            "node_modules/intl-tel-input/build/css/intlTelInput.css",
                            "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
                            "node_modules/primeng/resources/primeng.min.css"

                        ],
                        "scripts": [],
                        "main": "projects/healthnable-foundation-app/src/main.ts",
                        "commonChunk": false
                    },
                    "configurations": {
                        "production": {
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "500kB",
                                    "maximumError": "1MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kB",
                                    "maximumError": "4kB"
                                }
                            ],
                            "outputHashing": "all"

                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "healthnable-foundation-app:build:production"

                        },
                        "development": {
                            "buildTarget": "healthnable-foundation-app:build:development"
                        }
                    },
                    "defaultConfiguration": "development",
                    "options": {
                        "port": 4200,
                        "publicHost": "http://localhost:4200"

                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "projects/healthnable-foundation-app/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [{
                            "glob": "**/*",
                            "input": "projects/healthnable-foundation-app/public"
                        }],
                        "styles": [
                            "@angular/material/prebuilt-themes/azure-blue.css",
                            "./node_modules/primeng/resources/themes/lara-light-blue/theme.css",
                            "./node_modules/primeng/resources/primeng.min.css",
                            "node_modules/intl-tel-input/build/css/intlTelInput.css",
                            "projects/healthnable-foundation-app/src/styles.scss"

                        ],
                        "scripts": []
                    }
                }
            }
        }
    }
}